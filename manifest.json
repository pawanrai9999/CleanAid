{
  "manifest_version": 2,
  "name": "CleanAid",
  "description": "Block Unwanted sites.",
  "version": "0.1",
  "icons": { "16": "images/icon.png",
          "48": "images/icon.png",
          "128": "images/icon.png" },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/jquery.js", "js/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "CleanAid",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background_script": "background.js",
  "author": "Pawan Rai",
    "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "notifications"
  ],
  "version_name": "build rc1"
}
